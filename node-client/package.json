{
    "name": "@klsakura/rocketmq-grpc-sdk",
    "version": "1.0.4",
    "description": "Node.js client SDK for RocketMQ with gRPC proxy support",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types && npm run copy:proto && npm run fix:esm",
        "build:cjs": "tsc --module commonjs --outDir dist --target es2018",
        "build:esm": "tsc --module esnext --outDir dist-esm --target es2020 && mv dist-esm/index.js dist/index.mjs && rm -rf dist-esm",
        "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
        "copy:proto": "cp rocketmq.proto dist/",
        "fix:esm": "node scripts/fix-esm.js",
        "clean": "rm -rf dist dist-esm",
        "dev": "tsc --watch",
        "prepublishOnly": "npm run build",
        "test": "echo \"Error: no test specified\" && exit 1",
        "example:producer": "node examples/usage-demo.js producer",
        "example:consumer": "node examples/usage-demo.js consumer"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.9.0",
        "@grpc/proto-loader": "^0.7.8"
    },
    "devDependencies": {
        "@types/node": "^12.20.0",
        "typescript": "^5.8.3"
    },
    "keywords": [
        "rocketmq",
        "grpc",
        "nodejs",
        "typescript",
        "message-queue",
        "bytedance",
        "volcengine",
        "sdk",
        "client",
        "esm",
        "commonjs"
    ],
    "author": "hezhe",
    "license": "MIT",
    "files": [
        "dist/",
        "examples/",
        "README.md",
        "CHANGELOG.md"
    ],
    "engines": {
        "node": ">=12.20.0"
    }
}